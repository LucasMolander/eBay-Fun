file util

class FileUtil(object):
	def _loadFromFiles(only=None):
    if (only):
        with open(only, 'r') as f:
            return json.loads(f.read())

    nameToCards = {}

    for name in sets:
        with open(name, 'r') as f:
            cards = json.loads(f.read())

        nameToCards[name] = cards

    return nameToCards
